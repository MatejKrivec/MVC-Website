@{
}


@model Turneje

<form method="post" asp-action="SaveEditedTurneja" asp-route-newTurneja="@Model.turneja">
    <div>
        <label>Turneja:</label>
        <input type="text" asp-for="turneja" readonly />
    </div>
    <label>New Turneja:</label>
    <input type="text" name="newTurneja" />
    <div>
        <label>Clani:</label>
        <input type="text" asp-for="clani" />
    </div>
    <div>
        <label>Bandi:</label>
        <input type="text" asp-for="bendi" />
    </div>
    <div>
        <button type="submit">Save Changes</button>
    </div>
</form>